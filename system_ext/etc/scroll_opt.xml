    <DurationOpt id="duration">
        <param name = "maxCoef" value = "2" />
        <param name = "veloMax" value = "12000" />
        <param name = "veloMin" value = "5000" />
        <param name = "minCoef" value = "1.3" />
    </DurationOpt>

