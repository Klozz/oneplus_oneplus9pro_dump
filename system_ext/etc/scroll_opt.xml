<ScrollOptParameters>
    <param name = "enable" value = "true"/>
    <param name = "interpolatorStr" value="0.25,0.75,0.4,1" />
    <param name = "veloAccuCoef" value="1.5" />
    <param name = "maxAccuCoef" value = "4.5" />
    <param name = "accuCountThresh" value = "3"/>
    <param name = "flingAbortIntervalMax" value = "150"/>
